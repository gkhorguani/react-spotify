{
  "name": "react-spotify",
  "version": "1.0.0",
  "description": "ReactJS wrapper for spotify API",
  "main": "webpack.config.js",
  "scripts": {
    "start": "npm-run-all --parallel node-transpile webpack:watch",
    "test": "echo \"Error: no test specified\" && exit 1",
    "webpack:watch": "webpack -w",
    "webpack:dev": "webpack-dev-server --content-base public/",
    "node-server": "nodemon server.js",
    "node-transpile": "nodemon server.js --exec babel-node --presets es2015,stage-2 --plugins transform-object-rest-spread",
    "node-debug": "babel-node --inspect-brk server.js --presets es2015,stage-2",
    "scss-w": "node-sass src/scss  -w -o public/css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gkhorguani/react-spotify.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/gkhorguani/react-spotify/issues"
  },
  "homepage": "https://github.com/gkhorguani/react-spotify#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "eslint": "^4.16.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.6.1",
    "file-loader": "^1.1.6",
    "nodemon": "^1.14.11",
    "sass-loader": "^6.0.6",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.11.1"
  },
  "dependencies": {
    "apollo-cache-inmemory": "^1.1.9",
    "apollo-client": "^2.2.5",
    "apollo-link": "^1.2.1",
    "apollo-link-error": "^1.0.7",
    "apollo-link-http": "^1.5.2",
    "apollo-resolvers": "^1.0.3",
    "apollo-server-express": "^1.3.2",
    "axios": "^0.17.1",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "body-parser": "^1.18.2",
    "cookie-parser": "^1.4.3",
